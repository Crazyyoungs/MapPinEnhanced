<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/beta/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Include file="setView/setView.xml" />
    <Include file="optionsView/optionsView.xml" />


    <Script file="EditorWindow.lua" />
    <Frame name="MapPinEnhancedEditorWindowTemplate" mixin="MapPinEnhancedEditorWindowMixin"
        virtual="true"
        toplevel="true" parent="UIParent" enableMouse="true" movable="true" clampedToScreen="true"
        registerForDrag="LeftButton">
        <Size x="850" y="500" />
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Frames>
            <Frame parentKey="sideBar" inherits="MapPinEnhancedBaseFrameTemplate">
                <Size x="250" y="500" />
                <Anchors>
                    <Anchor point="TOPLEFT" />
                    <Anchor point="BOTTOMLEFT" />
                </Anchors>
            </Frame>
            <Frame parentKey="body" inherits="MapPinEnhancedBaseFrameTemplate">
                <Size x="1" y="1" />
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeKey="$parent.sideBar"
                        x="-7" />
                    <Anchor point="BOTTOMRIGHT" />
                </Anchors>
            </Frame>
            <Button parentKey="testbutton" inherits="BigRedExitButtonTemplate"
                frameStrata="HIGH">
                <Size x="16" y="16" />
                <Anchors>
                    <Anchor point="TOPRIGHT" x="2" y="2" />
                </Anchors>
                <Scripts>
                    <OnClick>
                        self:GetParent():Hide();
                    </OnClick>
                </Scripts>
            </Button>
        </Frames>
        <Scripts>
            <OnLoad method="OnLoad" />
        </Scripts>
    </Frame>
</Ui>
