{
    "workspace.ignoreDir": [
        ".github",
        ".vscode",
        "_old",
        "libs"
    ],
    "diagnostics.disable": [
        "undefined-global"
    ],
    "diagnostics.groupFileStatus": {
        "strong": "Any",
        "strict": "Any",
        "type-check": "Any",
        "luadoc": "Any",
        "unused": "Any",
        "unbalanced": "Any",
        "redefined": "Any",
        "ambiguity": "Any",
        "duplicate": "Any",
        "codestyle": "None"
    },
    "diagnostics.groupSeverity": {
        "strict": "Error",
        "unbalanced": "Error",
        "redefined": "Error",
        "strong": "Error",
        "type-check": "Error",
        "ambiguity": "Error",
        "unused": "Warning",
        "duplicate": "Error",
        "luadoc": "Warning",
        "codestyle": "Information"
    },
    "runtime.version": "Lua 5.1"
}
